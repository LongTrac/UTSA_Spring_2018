<?xml version="1.0" encoding="UTF-8"?>

<?import java.lang.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.*?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="application.controller.TransactionController">
   <children>
      <Pane layoutX="2.0" prefHeight="500.0" prefWidth="600.0">
         <children>
            <Button layoutX="14.0" layoutY="14.0" mnemonicParsing="false" onAction="#handle" text="Home" />
            <Label layoutX="156.0" layoutY="21.0" text="Transactions" textFill="#0fe404">
               <font>
                  <Font name="Cambria" size="52.0" />
               </font>
            </Label>
            <TableView fx:id="table" layoutX="29.0" layoutY="110.0" prefHeight="366.0" prefWidth="543.0">
              <columns>
                <TableColumn fx:id="date" prefWidth="163.0" text="Date" />
                <TableColumn fx:id="name" prefWidth="181.0" text="Name" />
                  <TableColumn fx:id="price" prefWidth="182.0" text="USD" />
              </columns>
            </TableView>
         </children>
      </Pane>
   </children>
</AnchorPane>
